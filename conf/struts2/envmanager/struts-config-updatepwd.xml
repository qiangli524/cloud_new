<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="pwdobj"
			 extends="global_package">
		<action name="pwdobj_*"
				class="updatepwdAction" method="{1}">
			<result name="success">/envmanager/updatepwd/listPwd.jsp</result>
			<result name="add">/envmanager/updatepwd/editPwd.jsp</result>
			<result name="update">/envmanager/updatepwd/editPwd.jsp</result>
			<result name="save">/envmanager/updatepwd/listPwd.jsp</result>
			<result name="del" >/envmanager/updatepwd/listPwd.jsp</result>
		
		</action>
			
	</package>
</struts>

