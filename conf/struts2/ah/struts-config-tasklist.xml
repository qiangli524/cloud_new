<struts>
	<package name="tasklist"
			 extends="global_package">
			 <action name="tasklist_*" class="taskListAction" method="{1}">
			 <result name="list">/tasklist/listTaskList.jsp </result>
			 <result name="add">/tasklist/addTaskList.jsp</result>
			 <result name ="update">/tasklist/updateTaskList.jsp</result>
			 <result name ="recordlist">/tasklist/queryTaskRecords.jsp</result>
			 <result name ="addRecord">/tasklist/addTaskRecord.jsp</result>
			 <result name ="updateRecord">/tasklist/updateTaskRecord.jsp</result>
			</action>
			
	</package>
	</struts>