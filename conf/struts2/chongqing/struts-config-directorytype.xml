<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="directoryType" extends="global_package">
		<action name="directoryType_*" class="directoryTypeAction" method="{1}">
			<result name="directoryTypeList">/chongqing/directoryType/directoryTypeList.jsp</result>
			<result name="directoryTypeToEdit">/chongqing/directoryType/directoryTypeEdit.jsp</result>
			<result name="directoryTypeSaved" type="redirect">directoryType_list.do</result>
			<result name="directoryTypedeleted" type="redirect">directoryType_list.do</result>
		</action>
	</package>
 </struts>
