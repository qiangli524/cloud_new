<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="appexcel"
			 extends="global_package">
		<action name="appexcel_*"
				class="appExcelAction" method="{1}">
			<result name="init">/component/excel/initAppExcel.jsp</result>
			<result name="initprocess">/component/excel/initAppProcessExcel.jsp</result>
			<result name="tabs">/component/excel/tabs.jsp</result>
		</action>
	</package>
</struts>