<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
 <package name="rule" extends="global_package">
		<action name="rule_*"
				class="ruleValidateAction" method="{1}">
			<result name="list">/component/tree/rule/listRule.jsp</result>
			<result name="edit">/component/tree/rule/editRule.jsp</result>
			<result name="redirect" type="redirect">configfile_list.do</result>
			
		</action>
 </package>

</struts>