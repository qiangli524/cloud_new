<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="configure"
			 extends="global_package">
		<action name="configure_*"
				class="configureAction" method="{1}">
			<result name="success">/envmanager/configure/listConfigure.jsp</result>
			<result name="add">/envmanager/configure/editConfigure.jsp</result>
			<result name="save">/envmanager/configure/listConfigure.jsp</result>
			<result name="update">/envmanager/configure/editConfigure.jsp</result>
			<result name="del">/envmanager/configure/listConfigure.jsp</result>
			
		</action>
			
	</package>
</struts>
