<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
 <package name="connect" extends="global_package">
		<action name="connectinfo_*" 
		        class="connectAction" method="{1}">
		    <result name="success">/yicloud/connect/listConnect.jsp</result>
			<result name="add">/yicloud/connect/addConnect.jsp</result>
			<result name="modify">/yicloud/connect/editConnect.jsp</result>
			<result name="redirect" type="redirect">connectinfo_listConnect.do</result>
		</action>
 </package>

</struts>