<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
 <package name="yiip" extends="global_package">
		<action name="yiip_*"
				class="tbYicloudAppIpAction" method="{1}">
			<result name="success">/yicloud/business/ip/listIpInfo.jsp</result>
			<result name="add">/yicloud/business/ip/addIpInfo.jsp</result>
			<result name="modify">/yicloud/business/ip/modIpInfo.jsp</result>
			<result name="redirect" type="redirect">yiip_listIp.do</result>
		</action>
 </package>
</struts>