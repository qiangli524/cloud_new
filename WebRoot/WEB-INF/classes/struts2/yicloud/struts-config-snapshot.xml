	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
 <package name="snapshot" extends="global_package">
		<action name="snapshot_*"
				class="snapshotAction" method="{1}">
			<result name="snapshotManager">/yicloud/entitytree/snapshotManager.jsp</result>
			<result name="executeSnapshot">/yicloud/entitytree/executeSnapshot.jsp</result>
		</action>
 </package>

</struts>