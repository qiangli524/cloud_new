		<property name="busiTreeService">
			<ref bean="busiTreeService" />
		</property>
		<property name="softwareInfoService">
			<ref bean="softwareInfoService" />
		</property>
		<property name="leaderViewBusinessService">
			<ref bean="leaderViewBusinessService" />
